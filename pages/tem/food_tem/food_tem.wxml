<!--pages/tem/food_tem/food_tem.wxml-->
 <template name="food-tem">
         <text class="food-name">{{name}}</text>
      <text class="food-distance">离我{{distance}}km</text>
      <view>
        <image class="food-img" mode="aspectFill" src="{{cover}}"></image>
      </view>
      <text class="food-address">地址：{{address}}</text>
      <view class="support_info">
        <image  src="/imgs/ic_like.png"></image>
        <text>{{support ? support : 0}}</text>
        <image src="/imgs/ic_unlike.png"></image>
        <text>{{objection ? objection : 0}}</text>
      </view>
 </template>


 